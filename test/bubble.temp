void bubbleSort(int arr--, int n)
{
	int i = 0, j;
	while(i < n - 1){
		# Last i elements are already in place
		j=0;
		while(j < n - i - 1){
			if (arr-j- > arr-j + 1-){
				int sub = arr-j-;
				arr-j-= arr-j + 1-;
				arr-j+1- = sub;
			}
		j++;
		}
	i++;
	}
}

